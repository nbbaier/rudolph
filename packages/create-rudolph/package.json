{
	"name": "@nbbaier/create-rudolph",
	"version": "0.1.2",
	"description": "Scaffold a new Advent of Code workspace with rudolph CLI (Early Release)",
	"type": "module",
	"author": "nbbaier",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nbbaier/rudolph.git"
	},
	"bugs": "https://github.com/nbbaier/rudolph/issues",
	"bin": {
		"create-rudolph": "dist/index.mjs"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsdown",
		"dev": "bun src/index.ts",
		"test": "bun test",
		"dev:new": "bun run src/index-new.ts",
		"test:new": "bun run src/index-new.ts",
		"debug:scaffold": "bun src/debug-scaffold.ts",
		"debug:cli": "DEBUG_SCAFFOLD=1 bun src/index.ts",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"advent-of-code",
		"aoc",
		"cli",
		"scaffold",
		"create"
	],
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"picocolors": "^1.1.1"
	},
	"devDependencies": {
		"@types/bun": "^1.1.13",
		"@types/node": "^22.9.0",
		"typescript": "^5.9.3",
		"tsdown": "^0.17.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"engines": {
		"bun": ">=1.3.3",
		"node": ">=18"
	}
}

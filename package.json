{
	"name": "rudolph",
	"version": "1.0.0",
	"description": "Advent of Code CLI - scaffold, run, and manage AoC solutions",
	"type": "module",
	"bin": {
		"rudolph": "./dist/index.mjs"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsdown && cp -r src/templates dist/templates",
		"dev": "bun src/index.ts",
		"prepublishOnly": "npm run build"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"advent-of-code",
		"aoc",
		"cli",
		"scaffold"
	],
	"license": "MIT",
	"dependencies": {
		"commander": "^14.0.2",
		"defuddle": "^0.6.6",
		"eta": "^4.4.1",
		"jsdom": "^25.0.1",
		"remark-frontmatter": "^5.0.0",
		"remark-normalize-headings": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-stringify": "^11.0.0",
		"remark-unlink": "^5.0.0",
		"unified": "^11.0.5",
		"yaml": "^2.8.2",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.10.0",
		"tsdown": "^0.17.0",
		"typescript": "^5.7.2"
	}
}

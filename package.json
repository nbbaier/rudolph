{
	"name": "@nbbaier/rudolph",
	"version": "0.2.0",
	"description": "Advent of Code CLI - scaffold, run, and manage AoC solutions",
	"type": "module",
	"author": "nbbaier",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nbbaier/rudolph.git"
	},
	"bugs": "https://github.com/nbbaier/rudolph/issues",
	"bin": {
		"rudolph": "dist/index.mjs"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsdown",
		"dev": "bun src/index.ts",
		"test": "bun test",
		"lint": "biome check .",
		"lint:fix": "biome check . --write",
		"format": "biome format .",
		"format:fix": "biome format . --write",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"bun": ">=1.3.3",
		"node": ">=18"
	},
	"keywords": [
		"advent-of-code",
		"aoc",
		"cli",
		"scaffold"
	],
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"arg": "^5.0.2",
		"chalk": "^5.3.0",
		"commander": "^14.0.2",
		"defuddle": "^0.6.6",
		"jsdom": "^24.1.0",
		"mdast": "^3.0.0",
		"remark-frontmatter": "^5.0.0",
		"remark-normalize-headings": "^4.0.0",
		"remark-parse": "^11.0.0",
		"remark-stringify": "^11.0.0",
		"remark-unlink": "^5.0.0",
		"tsx": "^4.19.0",
		"unified": "^11.0.5",
		"yaml": "^2.8.2",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/bun": "^1.3.3",
		"@types/jsdom": "^27.0.0",
		"@types/mdast": "^4.0.4",
		"@types/node": "^22.9.0",
		"tsdown": "^0.17.0",
		"typescript": "^5.9.3"
	}
}
